<h1> Our Amazing Products </h1>

<div class="row">
  <% @products.each do |product| %>
    <div class="col-md-4">
      <div class="thumbnail">
        <h2> <%= link_to "#{product.name}", "/products/#{product.id}" %></h2>
        <h3> PRICE: <%= product.price %> </h3>
        
        <h3> DESCRIPTION: <%= product.description %> </h3>

        <br>
      </div>
    </div>
  <%end%>
</div>

<p><%= @test_token %></p>
<p><%= @test_secret %></p>
